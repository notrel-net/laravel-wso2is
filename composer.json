{
    "name": "notrel/laravel-wso2is",
    "description": "A Laravel package for integrating with WSO2 Identity Server",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "wso2",
        "identity-server",
        "oauth",
        "authentication",
        "authorization"
    ],
    "authors": [
        {
            "name": "Don Mbelembe",
            "email": "bizroy678@gmail.com",
            "homepage": "https://github.com/donmbelembe",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Notrel\\LaravelWso2is\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {},
    "scripts": {
        "test": "pest",
        "test:unit": "pest --testsuite=Unit",
        "test:feature": "pest --testsuite=Feature",
        "test:integration": "phpunit --configuration phpunit.integration.xml",
        "test:all": [
            "@test",
            "@test:integration"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/notrel-net/laravel-wso2is/issues",
        "source": "https://github.com/notrel-net/laravel-wso2is"
    }
}